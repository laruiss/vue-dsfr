{
  "name": "@gouvminint/vue-dsfr",
  "type": "module",
  "version": "3.7.5",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dnum-mi/vue-dsfr.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./types/index.d.ts",
        "default": "./dist/vue-dsfr.js"
      }
    },
    "./dist/vue-dsfr.css": {
      "import": "./dist/vue-dsfr.css"
    },
    "./meta": {
      "types": "./meta-dts/index.d.ts",
      "import": "./meta/index.js"
    },
    "./styles": {
      "import": "./dist/vue-dsfr.css"
    }
  },
  "main": "./dist/vue-dsfr.umd.cjs",
  "module": "./dist/vue-dsfr.js",
  "types": "./types",
  "bin": {
    "vue-dsfr-icons": "./meta/custom-icon-collections-creator-bin.js"
  },
  "files": [
    "LICENSE",
    "README.md",
    "dist",
    "lib",
    "meta",
    "meta-dts",
    "types"
  ],
  "engines": {
    "node": ">=20.x.x",
    "npm": ">=10.x.x"
  },
  "publishConfig": {
    "@gouvminint:registry": "https://registry.npmjs.com",
    "access": "public"
  },
  "scripts": {
    "check-exports": "node ./ci/check-exports.mjs",
    "check-exports-ci": "node ./ci/check-exports.mjs --ci",
    "clean-dist": "rimraf dist lib",
    "prebuild": "run-s clean-dist",
    "build": "run-s build:vite type-gen build:meta",
    "build:vite": "vite build",
    "build:meta": "tsc -p tsconfig.meta.json",
    "demo": "vite",
    "build:demo": "vite -c vite.config.demo.js build",
    "dev": "storybook dev -p 6006",
    "vitest": "vitest",
    "coverage": "vitest run --coverage",
    "test-storybook": "cross-env test-storybook --url ${TEST_STORYBOOK_URL:-http://localhost:6006} --browsers firefox",
    "test": "run-s test:unit",
    "test:watch": "vitest",
    "test:unit": "vitest run",
    "format": "eslint . --fix",
    "lint": "eslint .",
    "lint:staged": "eslint ",
    "prepublishOnly": "run-s build",
    "semantic-release": "semantic-release",
    "type-gen": "vue-tsc -p tsconfig.app.json --declaration --emitDeclarationOnly",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "docs:dev": "cross-env VITEPRESS=true vitepress dev",
    "docs:build": "cross-env VITEPRESS=true vitepress build",
    "docs:build-only": "cross-env VITEPRESS=true vitepress build",
    "docs:preview": "cross-env VITEPRESS=true vitepress preview",
    "commitlint": "commitlint --edit"
  },
  "peerDependencies": {
    "@iconify/vue": "^4.2.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "dependencies": {
    "@gouvfr/dsfr": "~1.12.1",
    "focus-trap": "^7.6.4",
    "focus-trap-vue": "^4.0.3",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.16.0",
    "@chromatic-com/storybook": "^3.2.4",
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@iconify/vue": "^4.3.0",
    "@playwright/test": "1.49.1",
    "@storybook/addon-a11y": "^8.5.2",
    "@storybook/addon-docs": "^8.5.2",
    "@storybook/addon-essentials": "^8.5.2",
    "@storybook/addon-interactions": "^8.5.2",
    "@storybook/addon-links": "^8.5.2",
    "@storybook/addon-mdx-gfm": "^8.5.2",
    "@storybook/addon-themes": "^8.5.2",
    "@storybook/blocks": "^8.5.2",
    "@storybook/test": "^8.5.2",
    "@storybook/test-runner": "^0.21.0",
    "@storybook/theming": "^8.5.2",
    "@storybook/vue3": "^8.5.2",
    "@storybook/vue3-vite": "^8.5.2",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.12.0",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vitejs/plugin-vue-jsx": "^4.1.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "@vueuse/core": "^12.5.0",
    "@vueuse/shared": "^12.5.0",
    "@whyframe/core": "^0.1.12",
    "@whyframe/vue": "^0.1.7",
    "browserslist": "^4.24.4",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "commitlint": "^19.6.1",
    "cross-env": "^7.0.3",
    "eslint": "^9.19.0",
    "eslint-plugin-vue": "^9.32.0",
    "globby": "^14.0.2",
    "husky": "^9.1.7",
    "inquirer": "^12.3.3",
    "jsdom": "^26.0.0",
    "lightningcss": "^1.29.1",
    "lint-staged": "^15.4.3",
    "npm-run-all": "^4.1.5",
    "p-debounce": "^4.0.0",
    "pnpm": "^9.15.4",
    "publint": "^0.3.2",
    "regenerator-runtime": "^0.14.1",
    "rimraf": "^6.0.1",
    "semantic-release": "^24.2.1",
    "start-server-and-test": "^2.0.10",
    "storybook": "^8.5.2",
    "typescript": "~5.6.3",
    "vite": "^5.4.14",
    "vite-node": "^3.0.4",
    "vite-plugin-full-reload": "^1.2.0",
    "vitepress": "^1.6.3",
    "vitest": "^3.0.4",
    "vue-demi": "^0.14.10",
    "vue-tsc": "^2.2.0"
  },
  "lint-staged": {
    "*.{vue,js,jsx,ts,tsx}": "run-s check-exports-ci lint --"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "beta",
        "prerelease": true,
        "channel": "beta"
      },
      {
        "name": "next",
        "prerelease": true,
        "channel": "next"
      }
    ],
    "assets": [
      "dist/*.js",
      "dist/*.css"
    ]
  },
  "volta": {
    "node": "20.11.0"
  }
}
